  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^6.2.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^2.3.1",
    "istanbul": "^0.4.5",
    "mocha": "^3.5.2"
  },
  "engines": {
    "node": ">=0.12.0"
  },
  "maintainers": "commenthol <commenthol@gmail.com>"
