{
  "name": "date-chinese",
  "version": "1.0.3",
  "description": "Chinese Calendar",
  "keywords": [
    "calendar",
    "chinese",
    "chinese new year",
    "conversion",
    "gregorian",
    "japanese",
    "jieqi",
    "korean",
    "qingming",
    "sekki",
    "vietnamese",
    "zhongqi"
  ],
  "author": "commenthol <commenthol@gmail.com>",
  "main": "lib",
  "module": "lib",
  "jsnext:main": "src",
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/commenthol/date-chinese.git"
  },
  "scripts": {
